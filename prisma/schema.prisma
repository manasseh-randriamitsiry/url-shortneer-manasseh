generator client {
  provider = "prisma-client-js"
  output   = "../generated/prisma"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Url {
  id          Int      @id @default(autoincrement())
  originalUrl String
  shortCode   String   @unique
  createdAt   DateTime @default(now())
}
